body border border-dark\">\r\n                <form>\r\n                  <div class=\"form-row\">\r\n                    <div class=\"form-group col-md-12\">\r\n                      <label class=\"font-weight-bold\">จำนวนเงิน </label><br />\r\n                      <input\r\n                        v-model=\"amount\"\r\n                        type=\"number\"\r\n                        class=\"form-control\"\r\n                        placeholder=\"กรุณากรอกจำนวนเงิน\"\r\n                        required\r\n                      />\r\n                      <h6 class=\"text-danger mt-2 pl-2 font-weight-bold\">\r\n                        ฝากขั้นต่ำ 1 บาท\r\n                      </h6>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"row mt-3\">\r\n                    <div class=\"col-md-12 text-center\">\r\n                      <button\r\n                        @click.prevent=\"deposit\"\r\n                        class=\"\r\n                          btn btn-blue btn-lg\r\n                          rounded-pill\r\n                          text-center text-white\r\n                        \"\r\n                        style=\"\r\n                          background: linear-gradient(\r\n                            0deg,\r\n                            #007bff 0%,\r\n                            rgb(0 121 255 / 62%) 100%\r\n                          );\r\n                        \"\r\n                      >\r\n                        <i class=\"fas fa-check-circle\"></i> ทำรายการ\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport bankService from \"./../../../../services/bankService\";\r\nimport Swal from \"sweetalert2\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      amount: \"\