AYA,OAZA;AAaA,gBAAA,UAbA,GAaA,MAAA,CAAA,SAAA,GAAA,CAAA,GAAA,MAAA,CAAA,SAAA,GAAA,CAbA;AAcA,gBAAA,QAdA,GAcA,MAAA,CAAA,SAAA,GAAA,CAAA,GAAA,MAAA,CAAA,UAAA,GAAA,CAdA;AAAA;AAgBA,gBAAA,SAhBA,GAgBA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA;AADA,iBAhBA;AAAA;AAAA,uBAmBA,iBAAA,CAAA,WAAA,CAAA,SAAA,EAAA,UAAA,EAAA,QAAA,CAnBA;;AAAA;AAmBA,gBAAA,IAnBA;AAoBA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;;AACA,oBAAA,MAAA,CAAA,IAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,qBAAA,EAAA;AACA,oBAAA,QAAA,EAAA,WADA;AAEA,oBAAA,OAAA,EAAA,IAFA;AAGA,oBAAA,IAAA,EAAA,IAHA;AAIA,oBAAA,GAAA,EAAA;AAJA,mBAAA;AAMA;;AA7BA;AAAA;;AAAA;AAAA;AAAA;;AA+BA,gBAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,gBAAA,EAAA;AACA,kBAAA,QAAA,EAAA,WADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AA/BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoCA;AAnJA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"weekDeposit\">\r\n    <div class=\"page-inner\">\r\n      <div class=\"page-header\">\r\n        <h4 class=\"page-title\"><i class=\"fas fa-book\"></i>ฝาก 7 วัน</h4>\r\n      </div>\r\n      <div class=\"row\">\r\n        <div class=\"col-md-5\">\r\n          <div class=\"card\">\r\n            <div class=\"card-body\">\r\n              <div class=\"card-body\">\r\n                <form\r\n                  class=\"form-inline justify-content-center\"\r\n                  v-on:submit.prevent=\"weekDeposit\"\r\n                >\r\n                  <div class=\"form-group\">\r\n                    <label for=\"email2 \">\r\n                      Username :\r\n                      <span class=\"required-label\">*</span>\r\n                    </label>\r\n                    <input\r\n                      class=\"form-control ml-4\"\r\n                      id=\"email2\"\r\n                      placeholder=\"Username\"\r\n                      v-model=\"username\"\r\n                      required\r\n                    />\r\n\r\n                    <button class=\"btn btn-info ml-2\" type=\"submit\">\r\n                      <i class=\"fa fa-search\" aria-hidden=\"true\"></i>\r\n                      ค้นหา\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-7\">\r\n          <div class=\"card\">\r\n             <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">\r\n                    <i class=\"fa fa-calendar\" aria-hidden=\"true\"></i>\r\n                    จากวันที่\r\n                  </label>\r\n                  <VueCtkDateTimePicker\r\n                    :only-date=\"true\"\r\n                    v-model=\"yesterdate\"\r\n                    format=\"YYYY-MM-DD\"\r\n                    formatted=\"DD-MM-YYYY\"\r\n                    :no-label=\"true\"\r\n                    locale=\"th-TH\"\r\n                    label=\"yesterday\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">\r\n                    <i class=\"fa fa-clock-o\" aria-hidden=\"true\"></i> เวลา\r\n                  </label>\r\n                  <VueCtkDateTimePicker\r\n                    v-model=\"yestertime\"\r\n                    :only-time=\"true\"\r\n                    format=\"HH:mm:ss\"\r\n                    formatted=\"HH:mm\"\r\n                    :no-label=\"true\"\r\n                    label=\"Time\"\r\n                    disabled\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">\r\n                    <i class=\"fa fa-calendar\" aria-hidden=\"true\"></i>\r\n                    ถึงวันที่\r\n                  </label>\r\n                  <VueCtkDateTimePicker\r\n                    :only-date=\"true\"\r\n                    v-model=\"todate\"\r\n                    format=\"YYYY-MM-DD\"\r\n                    formatted=\"DD-MM-YYYY\"\r\n                    :no-label=\"true\"\r\n                    locale=\"th-TH\"\r\n                    label=\"today\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">\r\n                    <i class=\"fa fa-clock-o\" aria-hidden=\"true\"></i>\r\n                    เวลา\r\n                  </label>\r\n                  <VueCtkDateTimePicker\r\n                    v-model=\"totime\"\r\n                    :only-time=\"true\"\r\n                    format=\"HH:mm:ss\"\r\n                    formatted=\"HH:mm\"\r\n                    :no-label=\"true\"\r\n                    label=\"Time\"\r\n                    disabled\r\n                  />\r\n                </div>\r\n\r\n                <div class=\"col-md-1.5\">\r\n                  <br />\r\n                  <button\r\n                    class=\"btn btn-primary btn-sm ml-1\"\r\n                    v-on:click.prevent=\"getDataParam()\"\r\n                  >\r\n                    <i class=\"fas fa-search\"></i>\r\n                    ค้นหา\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-secondary btn-sm ml-1\"\r\n                    v-on:click.prevent=\"getYesterday()\"\r\n                  >\r\n                    เมื่อวาน\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-warning btn-sm ml-1\"\r\n                    v-on:click.prevent=\"getToday()\"\r\n                  >\r\n                    วันนี้\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n            <div class=\"table-responsive\">\r\n              <table\r\n                id=\"basic-datatables\"\r\n                class=\"display table table-striped table-hover table-bordered\"\r\n              >\r\n                <thead style=\"text-align: center\">\r\n                  <tr style=\"text-align: center\">\r\n                    <th>ชื่อผู้ใช้</th>\r\n                    <th>โบนัส</th>\r\n                    <th>เครดิตก่อนเติม</th>\r\n                    <th>เครดิตหลังเติม</th>\r\n                    <th>เวลา</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody style=\"text-align: center\"  >\r\n                  <tr v-for=\"data in week\" v-bind:key=\"data.uuid\">\r\n                    <td>{{data.username}}</td>\r\n                    <td>\r\n                      <div v-if=\"data.bonus_credit == null\">\r\n                          <span >0</span>\r\n                      </div>\r\n                      <div v-if=\"data.bonus_credit != null\">\r\n                          <span >{{ data.bonus_credit | formatNumber}}</span>\r\n                      </div>\r\n                    </td>\r\n                    <td>{{data.credit_before |formatNumber}}</td>\r\n                    <td>{{data.credit_after |formatNumber}}</td>\r\n                    <td>{{data.updatedAt | formatDate}}</td>\r\n                  </tr>\r\n                </tbody>\r\n                  <tbody>\r\n                    <tr v-if=\"week.length == '0'\">\r\n                      <td colspan=\"7\" class=\"text-center text-danger\">\r\n                        <i class=\"fas fa-times\"></i>\r\n                        กรอกชื่อผู้ใช้เพื่อค้นหาข้อมูล !\r\n                      </td>\r\n                    </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport memberTransaction from '../../../services/memberTransaction'\r\nimport moment from \"moment\";import UserService from \"@/services/UserService\";\r\n\r\nexport default {\r\n   data() {\r\n    return {\r\n        yesterdate: \"\",\r\n        yestertime: \"\",\r\n        todate: \"\",\r\n        totime: \"\",\r\n        username: \"\",\r\n        week:[]\r\n    };\r\n   },\r\n     mounted() {\r\nthis.checkAuth();\r\n\r\n        this.moment();\r\n  },\r\n    methods: {\r\n      async checkAuth(){\r\n       const res = await UserService.getProfile().catch(function (error) {\r\n        if(error.response.status === 401 || error.response.status === 500){\r\n      const removeToken = localStorage.removeItem(\"access_token\");\r\n        if (removeToken == null) {\r\n        this.$router.push(\"/login\");\r\n      }\r\n      }\r\n      });\r\n      res.logout\r\n    },\r\n\r\n    async weekDeposit() {\r\n      this.moment();\r\n      this.yesterdate = moment()\r\n        .locale(\"th\")\r\n        .subtract(1, \"days\")\r\n        .format(\"YYYY-MM-DD\");\r\n      this.yestertime = moment()\r\n        .locale(\"th\")\r\n        .subtract(1, \"days\")\r\n        .format(\"11:00:00\");\r\n      this.todate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n      this.totime = moment().locale(\"th\").format(\"11:00:00\");\r\n      this.todaydate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n      this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n      this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n      try {\r\n        const sevenForm = {\r\n          username: this.username,\r\n        };\r\n        const resp = await memberTransaction.weekDeposit(sevenForm,start_date,end_date);\r\n          console.log(resp);\r\n          this.week = resp.data.data\r\n          console.log(this.week);\r\n      } catch (error) {\r\n         this.$toast.error('ไม่มีผู้ใช้นี้', {\r\n          position: \"top-right\",\r\n          timeout: 1500,\r\n        });\r\n      }\r\n    },\r\n    async getDataParam(){\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const start_date = this.yesterdate + t + this.yestertime + z;\r\n      const end_date = this.todate + t + this.totime + z;\r\n      try {\r\n        const sevenForm = {\r\n          username: this.username,\r\n        };\r\n        const resp = await memberTransaction.weekDeposit(sevenForm,start_date,end_date);\r\n          console.log(resp);\r\n          this.week = resp.data.data\r\n           if (this.week.length <= 0) {\r\n            this.$toast.error(\"ไม่มีข้อมูลในวันนี้\", {\r\n              position: \"top-right\",\r\n              timeout: 1500,\r\n              icon: true,\r\n              rtl: false,\r\n            });\r\n          }\r\n      } catch (error) {\r\n         this.$toast.error('ไม่มีผู้ใช้นี้', {\r\n          position: \"top-right\",\r\n          timeout: 1500,\r\n        });\r\n      }\r\n      \r\n    },\r\n    async getToday() {\r\n      this.todaydate = moment()\r\n        .locale(\"th\")\r\n        .format(\"YYYY-MM-DD\");\r\n      this.todaytime = moment()\r\n        .locale(\"th\")\r\n        .format(\"00:00:00\");\r\n      this.todaytime2 = moment()\r\n        .locale(\"th\")\r\n        .format(\"23:59:59\");\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n        try {\r\n        const sevenForm = {\r\n          username: this.username,\r\n        };\r\n        const resp = await memberTransaction.weekDeposit(sevenForm,start_date,end_date);\r\n          console.log(resp);\r\n          this.week = resp.data.data\r\n           if (this.week.length <= 0) {\r\n            this.$toast.error(\"ไม่มีข้อมูลในวันนี้\", {\r\n              position: \"top-right\",\r\n              timeout: 1500,\r\n              icon: true,\r\n              rtl: false,\r\n            });\r\n          }\r\n      } catch (error) {\r\n         this.$toast.error('ไม่มีผู้ใช้นี้', {\r\n          position: \"top-right\",\r\n          timeout: 1500,\r\n        });\r\n      }\r\n    },\r\n    async getYesterday() {\r\n      this.todaydate = moment()\r\n        .locale(\"th\")\r\n        .subtract(1, \"days\")\r\n        .format(\"YYYY-MM-DD\");\r\n      this.todaytime2 = moment()\r\n        .locale(\"th\")\r\n        .format(\"23:59:59\");\r\n      this.todaytime = moment()\r\n        .locale(\"th\")\r\n        .format(\"00:00:00\");\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n      try {\r\n        const sevenForm = {\r\n          username: this.username,\r\n        };\r\n        const resp = await memberTransaction.weekDeposit(sevenForm,start_date,end_date);\r\n          console.log(resp);\r\n          this.week = resp.data.data\r\n           if (this.week.length <= 0) {\r\n            this.$toast.error(\"ไม่มีข้อมูลในวันนี้\", {\r\n              position: \"top-right\",\r\n              timeout: 1500,\r\n              icon: true,\r\n              rtl: false,\r\n            });\r\n          }\r\n      } catch (error) {\r\n         this.$toast.error('ไม่มีผู้ใช้นี้', {\r\n          position: \"top-right\",\r\n          timeout: 1500,\r\n        });\r\n      }\r\n    },\r\n    \r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n.table .thead-custom th {\r\n    color: #fff;\r\n    background-color: #4871f7 ;\r\n    border-color: #32383e;\r\n}\r\n</style>"],"sourceRoot":"src/components/pages/weekDeposit"}]}