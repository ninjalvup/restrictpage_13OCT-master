AACA,sBAAA,KAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAAA,IAAA,KAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,wBAAA,WAAA,GAAA,YAAA,CAAA,UAAA,CAAA,cAAA,CAAA;;AACA,wBAAA,WAAA,IAAA,IAAA,EAAA;AACA,2BAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA;AACA,iBAPA,CADA;;AAAA;AACA,gBAAA,GADA;AASA,gBAAA,GAAA,CAAA,MAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAXA;AAaA,IAAA,eAbA,6BAaA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,EAFA,CAGA;AACA;AACA;AAEA;AACA;AACA;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAnBA;AAAA,uBAoBA,SAAA,CAAA,OAAA,CAAA,KAAA,CAAA,IAAA,CApBA;;AAAA;AAoBA,gBAAA,IApBA;;AAqBA,gBAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,0BAAA,EAAA;AACA,kBAAA,QAAA,EAAA,WADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AAIA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AA3BA;AAAA;;AAAA;AAAA;AAAA;;AA6BA,gBAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,aAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,OAAA,EAAA;AACA,kBAAA,QAAA,EAAA,WADA;AAEA,kBAAA,OAAA,EAAA;AAFA,iBAAA;;AA7BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkCA,KA/CA;AAgDA,IAAA,YAhDA,0BAgDA;AACA,UAAA,SAAA,GACA,IAAA,CAAA,MAAA,GAAA,QAAA,CAAA,EAAA,EAAA,SAAA,CAAA,CAAA,EAAA,CAAA,IACA,IAAA,CAAA,MAAA,GAAA,QAAA,CAAA,EAAA,EAAA,SAAA,CAAA,CAAA,EAAA,CAAA,CAFA;AAGA,WAAA,IAAA,CAAA,MAAA,GAAA,SAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA,CAAA,MAAA;AAEA,aAAA,SAAA;AACA;AAxDA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div id=\"oldMember\">\r\n    <div class=\"page-inner\">\r\n      <div class=\"page-header\">\r\n        <br />\r\n        <h4 class=\"page-title\">\r\n          <i class=\"fa fa-user\" aria-hidden=\"true\"></i> ลูกค้าเก่า\r\n        </h4>\r\n        <ul class=\"breadcrumbs\"></ul>\r\n      </div>\r\n\r\n      <div class=\"col-md-12\">\r\n        <div class=\"col-md-12\">\r\n          <!-- <topDashboard /> -->\r\n        </div>\r\n        <div class=\"col-md-12 row\">\r\n          <div class=\"col-md-12\"></div>\r\n          <form v-on:submit.prevent=\"oldMemberUpdate\" class=\"col-md-12\">\r\n            <div class=\"col-md-12\">\r\n              <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                  <div class=\"column\">\r\n                    <div class=\"row\">\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>Username ลูกค้าเก่า</h4>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"ชื่อผู้ใช้งานเก่า\"\r\n                          v-model.trim=\"form.username\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                    <hr />\r\n                    <div class=\"row\">\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>สถานะลูกค้า</h4>\r\n                        <select\r\n                          class=\"form-control\"\r\n                          v-model=\"form.new_member\"\r\n                          required\r\n                        >\r\n                          <option selected disabled value>\r\n                            เลือกสถานะลูกค้า\r\n                          </option>\r\n                          <option value=\"1\">ลูกค้าใหม่</option>\r\n                          <option value=\"0\">ลูกค้าเก่า</option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>วันที่ลงทะเบียน</h4>\r\n                        <VueCtkDateTimePicker\r\n                          v-model=\"form.register_date\"\r\n                          required\r\n                          format=\"YYYY-MM-DD HH:mm:ss\"\r\n                          formatted=\"DD/MM/YYYY HH:mm\"\r\n                          :no-label=\"true\"\r\n                          label=\"เลือกวันที่\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>Prefix</h4>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"prefix\"\r\n                          v-model.trim=\"form.prefix\"\r\n                          required\r\n                          disabled\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"row\">\r\n                      <div class=\"card-body col-md-3\">\r\n                        <h4>หมายเลขโทรศัพท์ของลูกค้า</h4>\r\n                        <input\r\n                          type=\"number\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"หมายเลขโทรศัพท์\"\r\n                          v-model.trim=\"form.tel\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                      <div class=\"card-body col-md-3\">\r\n                        <h4>รหัสผ่าน</h4>\r\n                        <input\r\n                          type=\"password\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"รหัสผ่านผู้ใช้งานเก่า\"\r\n                          v-model.trim=\"form.password\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                      <div class=\"card-body col-md-3\">\r\n                        <h4>ชื่อ</h4>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"ชื่อตามสมุดบัญชี\"\r\n                          v-model.trim=\"form.first_name\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                      <div class=\"card-body col-md-3\">\r\n                        <h4>นามสกุล</h4>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"นามสกุลตามสมุดบัญชี\"\r\n                          v-model.trim=\"form.last_name\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                    <hr />\r\n                    <div class=\"row\">\r\n                      <div class=\"card-body col-md-6\">\r\n                        <h4>ธนาคาร</h4>\r\n                        <select\r\n                          class=\"form-control\"\r\n                          v-model=\"form.bank_name\"\r\n                          required\r\n                        >\r\n                          <option selected disabled value>เลือกธนาคาร</option>\r\n                          <option value=\"kbnk\">ธนาคารกสิกรไทย</option>\r\n                          <option value=\"ktba\">ธนาคารกรุงไทย</option>\r\n                          <option value=\"scb\">ธนาคารไทยพาณิชย์</option>\r\n                          <option value=\"bay\">ธนาคารกรุงศรีอยุธยา</option>\r\n                          <option value=\"bbla\">ธนาคารกรุงเทพ</option>\r\n                          <option value=\"gsb\">ธนาคารออมสิน</option>\r\n                          <option value=\"promptpay\">พร้อมเพย์</option>\r\n                          <option value=\"tmb\">ธนาคารทหารไทย</option>\r\n                          <!-- <option value=\"truemoney\">Truemoney Wallet</option> -->\r\n                          <option value=\"BAAC\">\r\n                            ธนาคารเพื่อการเกษตรและสหกรณ์การเกษตร\r\n                          </option>\r\n                          <option value=\"ICBC\">ธนาคารไอซีบีซี (ไทย)</option>\r\n                          <option value=\"TCD\">\r\n                            ธนาคารไทยเครดิตเพื่อรายย่อย\r\n                          </option>\r\n                          <option value=\"CITI\">ธนาคารซิตี้แบงก์</option>\r\n                          <option value=\"SCBT\">\r\n                            ธนาคารสแตนดาร์ดชาร์เตอร์ด (ไทย)\r\n                          </option>\r\n                          <option value=\"CIMB\">ธนาคารซีไอเอ็มบีไทย</option>\r\n                          <option value=\"UOB\">ธนาคารยูโอบี</option>\r\n                          <option value=\"HSBC\">\r\n                            ธนาคารเอชเอสบีซี ประเทศไทย\r\n                          </option>\r\n                          <option value=\"MIZUHO\">ธนาคารมิซูโฮ คอร์ปอเรต</option>\r\n                          <option value=\"GHB\">ธนาคารอาคารสงเคราะห์</option>\r\n                          <option value=\"LHBANK\">\r\n                            ธนาคารแลนด์ แอนด์ เฮ้าส์\r\n                          </option>\r\n                          <option value=\"TBANK\">ธนาคารธนชาต</option>\r\n                          <option value=\"TISCO\">ธนาคารทิสโก้</option>\r\n                          <option value=\"KKP\">ธนาคารเกียรตินาคิน</option>\r\n                          <option value=\"IBANK\">\r\n                            ธนาคารอิสลามแห่งประเทศไทย\r\n                          </option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"card-body col-md-6\">\r\n                        <h4>หมายเลขบัญชีธนาคาร</h4>\r\n                        <input\r\n                          type=\"number\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"หมายเลขบัญชี\"\r\n                          v-model.trim=\"form.bank_number\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                    <hr />\r\n                    <div class=\"row\">\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>การรับโบนัส</h4>\r\n                        <select\r\n                          class=\"form-control\"\r\n                          v-model=\"form.bonus\"\r\n                          required\r\n                        >\r\n                          <option selected disabled value>เลือกโบนัส</option>\r\n                          <option value=\"1\">รับโบนัส</option>\r\n                          <option value=\"0\">ไม่รับโบนัส</option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>รู้จักผ่าน</h4>\r\n                        <select\r\n                          class=\"form-control\"\r\n                          id=\"know_us\"\r\n                          name=\"know_us\"\r\n                          v-model=\"form.know_us\"\r\n                          required\r\n                        >\r\n                          <option selected disabled value>\r\n                            รู้จักเราผ่านช่องทางใด\r\n                          </option>\r\n                          <option value=\"facebook\">Facebook</option>\r\n                          <option value=\"line\">Line</option>\r\n                          <option value=\"instagram\">Instagram</option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"card-body col-md-4\">\r\n                        <h4>Line ID</h4>\r\n                        <input\r\n                          type=\"text\"\r\n                          class=\"form-control\"\r\n                          aria-describedby=\"helpId\"\r\n                          placeholder=\"ไลน์ไอดี\"\r\n                          v-model.trim=\"form.line_id\"\r\n                          required\r\n                        />\r\n                      </div>\r\n                    </div>\r\n                    <button\r\n                      type=\"submit\"\r\n                      class=\"btn btn-success mr-1 text-white float-right\"\r\n                    >\r\n                      <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\r\n                      ยืนยัน\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport oldMember from \"../../../services/old-member\";\r\nimport UserService from \"@/services/UserService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      prefixgen: \"\",\r\n\r\n      form: {\r\n        username: \"\",\r\n        password: \"\",\r\n        first_name: \"\",\r\n        last_name: \"\",\r\n        tel: \"\",\r\n        bank_number: \"\",\r\n        bank_name: \"\",\r\n        line_id: \"\",\r\n        know_us: \"\",\r\n        bonus: \"\",\r\n        new_member: \"\",\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.randomPrefix();\r\n  },\r\n  methods: {\r\n    async checkAuth() {\r\n      const res = await UserService.getProfile().catch(function (error) {\r\n        if (error.response.status === 401 || error.response.status === 500) {\r\n          const removeToken = localStorage.removeItem(\"access_token\");\r\n          if (removeToken == null) {\r\n            this.$router.push(\"/login\");\r\n          }\r\n        }\r\n      });\r\n      res.logout;\r\n    },\r\n\r\n    async oldMemberUpdate() {\r\n      try {\r\n        console.log(this.form);\r\n        // const oldmemberForm = {\r\n        //   username: this.form.username,\r\n        //   password: this.form.password,\r\n\r\n        //   first_name: this.form.first_name,\r\n        //   last_name: this.form.last_name,\r\n        //   tel: this.form.tel,\r\n\r\n        //   bank_number: this.form.bank_number,\r\n        //   bank_name: this.form.bank_name,\r\n        //   line_id: this.form.line_id,\r\n        //   know_us: this.form.know_us,\r\n        //   bonus: this.form.bonus,\r\n        //   register_date: this.form.register_date,\r\n        //   prefix: this.prefixgen,\r\n        //   new_member: this.form.new_member,\r\n        // };\r\n        const resp = await oldMember.oldUser(this.form);\r\n        this.$toast.success(\"เพิ่มข้อมูลเรียบร้อยแล้ว\", {\r\n          position: \"top-right\",\r\n          timeout: 1500,\r\n        });\r\n        this.$router.push(\"/user\");\r\n\r\n        console.log(resp);\r\n      } catch (error) {\r\n        this.$toast.error(error.response.data.error.message, {\r\n          position: \"top-right\",\r\n          timeout: 1500,\r\n        });\r\n      }\r\n    },\r\n    randomPrefix() {\r\n      let prefixgen =\r\n        Math.random().toString(36).substring(2, 6) +\r\n        Math.random().toString(36).substring(2, 4);\r\n      this.form.prefix = prefixgen;\r\n      console.log(this.form.prefix);\r\n\r\n      return prefixgen;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/pages/oldMember"}]}