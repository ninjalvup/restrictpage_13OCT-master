CA,WAAA,SAAA,GAAA,OAAA,GAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AACA,UAAA,CAAA,GAAA,GAAA;AACA,UAAA,CAAA,GAAA,EAAA;AACA,UAAA,UAAA,GAAA,KAAA,SAAA,GAAA,CAAA,GAAA,KAAA,SAAA,GAAA,CAAA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,GAAA,CAAA,GAAA,KAAA,UAAA,GAAA,CAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,YAAA,OAAA,yCAAA,UAAA,uBAAA,QAAA,eAFA;AAGA,QAAA,OAAA,EAAA;AACA,0BAAA,kBADA;AAEA,UAAA,aAAA,EAAA,YAAA,YAAA,CAAA,OAAA,CAAA,cAAA;AAFA;AAHA,OAAA,CAAA,CAQA,IARA,CAQA,UAAA,QAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AAEA,QAAA,MAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAdA,EAeA,KAfA,CAeA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAjBA;AAkBA,KA7GA;AA8GA,IAAA,YA9GA,0BA8GA;AACA,WAAA,MAAA,CAAA,OAAA,CAAA,wBAAA,EAAA;AACA,QAAA,QAAA,EAAA,WADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA;AAIA,KAnHA;AAoHA,IAAA,gBApHA,8BAoHA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,4BAAA,EAAA;AACA,QAAA,QAAA,EAAA,WADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA;AAIA;AAzHA;AAnEA,CAAA","sourcesContent":["<template>\r\n  <div id=\"User\">\r\n    <div class=\"page-inner\">\r\n      <div class=\"page-header\">\r\n        <h4 class=\"page-title\"><i class=\"fas fa-list-alt\"></i> รายงานการฝาก</h4>\r\n      </div>\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-2\">\r\n                <label for=\"datetime\">\r\n                  <i class=\"fa fa-calendar\" aria-hidden=\"true\"></i>\r\n                  จากวันที่</label\r\n                >\r\n                <VueCtkDateTimePicker\r\n                  :only-date=\"true\"\r\n                  v-model=\"yesterdate\"\r\n                  format=\"YYYY-MM-DD\"\r\n                  formatted=\"DD-MM-YYYY\"\r\n                  :no-label=\"true\"\r\n                  locale=\"th-TH\"\r\n                  Label=\"yesterday\"\r\n                />\r\n              </div>\r\n              <div class=\"col-md-2\">\r\n                <label for=\"datetime\">\r\n                  <i class=\"fa fa-clock-o\" aria-hidden=\"true\"></i>\r\n                  เวลา</label\r\n                >\r\n                <VueCtkDateTimePicker\r\n                  v-model=\"yestertime\"\r\n                  :only-time=\"true\"\r\n                  format=\"HH:mm:ss\"\r\n                  formatted=\"HH:mm\"\r\n                  :no-label=\"true\"\r\n                  label=\"Time\"\r\n                />\r\n              </div>\r\n              <div class=\"col-md-2\">\r\n                <label for=\"datetime\">\r\n                  <i class=\"fa fa-calendar\" aria-hidden=\"true\"></i>\r\n                  ถึงวันที่</label\r\n                >\r\n                <VueCtkDateTimePicker\r\n                  :only-date=\"true\"\r\n                  v-model=\"todate\"\r\n                  format=\"YYYY-MM-DD\"\r\n                  formatted=\"DD-MM-YYYY\"\r\n                  :no-label=\"true\"\r\n                  locale=\"th-TH\"\r\n                  Label=\"today\"\r\n                />\r\n              </div>\r\n              <div class=\"col-md-2\">\r\n                <label for=\"datetime\">\r\n                  <i class=\"fa fa-clock-o\" aria-hidden=\"true\"></i>\r\n                  เวลา</label\r\n                >\r\n                <VueCtkDateTimePicker\r\n                  v-model=\"totime\"\r\n                  :only-time=\"true\"\r\n                  format=\"HH:mm:ss\"\r\n                  formatted=\"HH:mm\"\r\n                  :no-label=\"true\"\r\n                  label=\"Time\"\r\n                />\r\n              </div>\r\n              <div class=\"col-md-2\">\r\n                <label for=\"datetime\">กรุณากรอกข้อมูลที่ต้องการ</label>\r\n                <input\r\n                  class=\"form-control\"\r\n                  id=\"email2\"\r\n                  placeholder=\"ข้อมูลที่ต้องการค้นหา\"\r\n                  v-model=\"username\"\r\n                />\r\n              </div>\r\n              <div class=\"col-md-1.5\">\r\n                <br />\r\n                <button\r\n                  class=\"btn btn-primary btn-sm ml-1\"\r\n                  v-on:click.prevent=\"getDataParam()\"\r\n                >\r\n                  <i class=\"fa fa-search\" aria-hidden=\"true\"></i>\r\n                  ค้นหา\r\n                </button>\r\n                <button\r\n                  class=\"btn btn-secondary btn-sm ml-1\"\r\n                  v-on:click.prevent=\"getYesterday()\"\r\n                >\r\n                  เมื่อวาน\r\n                </button>\r\n                <button\r\n                  class=\"btn btn-warning btn-sm ml-1\"\r\n                  v-on:click.prevent=\"getToday()\"\r\n                >\r\n                  วันนี้\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <div class=\"card\">\r\n            <div class=\"card-header\">\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <h3 class=\"card-title\">ข้อมูลรายงาน: การฝาก</h3>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div>\r\n              <div class=\"table-responsive\">\r\n                <table\r\n                  id=\"basic-datatables\"\r\n                  class=\"display table table-striped table-hover table-bordered\"\r\n                >\r\n                  <thead style=\"text-align: center\">\r\n                    <!-- <tr class=\"table table-bordered\">\r\n                      <th colspan=\"2\">รายการถอนทั้งหมด(x)</th>\r\n                      <th>xx,xxx.xx</th>\r\n                      <th colspan=\"8\"></th>\r\n                    </tr> -->\r\n                    <tr>\r\n                      <th>#</th>\r\n                      <th>ธนาคาร</th>\r\n                      <th>Username</th>\r\n                      <th>ยอดเงิน</th>\r\n                      <th>เวลาทำรายการ</th>\r\n\r\n                      <th>โบนัส</th>\r\n                      <th>ประเภท</th>\r\n                      <th>เครดิตก่อนเติม</th>\r\n                      <th>เครดิตหลังเติม</th>\r\n                      <th>ทำรายการโดย</th>\r\n                      <th>หมายเหตุ</th>\r\n                      <th>ref.</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody style=\"text-align: center\">\r\n                    <tr\r\n                      v-for=\"(data, index) in depositreport\"\r\n                      v-bind:key=\"data.id\"\r\n                    >\r\n                      <td>{{ index + 1 }}</td>\r\n                      <td>\r\n                        <div class=\"avatar mt-3\">\r\n                          <img\r\n                            :src=\"\r\n                              require(`../../../../assets/bank_icon/${data.member_account_banks[0].bank_name.toLowerCase()}.png`)\r\n                            \"\r\n                            class=\"icon\"\r\n                            width=\"40\"\r\n                          />\r\n                        </div>\r\n                        <div>\r\n                          <b>\r\n                            <a\r\n                              href=\"#/reportdeposit\"\r\n                              @click=\"getClipboardBank\"\r\n                              v-clipboard:copy=\"\r\n                                data.member_account_banks[0].bank_number\r\n                              \"\r\n                              >{{ data.member_account_banks[0].bank_number }}</a\r\n                            ></b\r\n                          >\r\n                        </div>\r\n                        <div>\r\n                          ({{ data.member_account_banks[0].bank_account_name }})\r\n                        </div>\r\n                      </td>\r\n                      <td>\r\n                        <b>\r\n                          <a\r\n                            href=\"#/reportdeposit\"\r\n                            @click=\"getClipboard\"\r\n                            v-clipboard:copy=\"data.username\"\r\n                            >{{ data.username }}</a\r\n                          ></b\r\n                        >\r\n                      </td>\r\n                      <td>{{ data.amount | formatCredit }}</td>\r\n                      <td>{{ data.bank_time }}</td>\r\n\r\n                      <td>{{ data.bonus_credit }}</td>\r\n                      <td>\r\n                        <div v-if=\"data.transaction_type == '1'\">\r\n                          <span class=\"badge badge-info\">ฝาก</span>\r\n                        </div>\r\n                        <div v-if=\"data.transaction_type == '2'\">\r\n                          <span class=\"badge badge-warning\">ถอน</span>\r\n                        </div>\r\n                      </td>\r\n\r\n                      <td>\r\n                        <div v-if=\"data.credit_before === null\">0</div>\r\n                        <div v-else>\r\n                          <span> {{ data.credit_before.toFixed(2) }}</span>\r\n                        </div>\r\n                      </td>\r\n                      <td>\r\n                        <span>{{ data.credit_after | formatNumber }}</span>\r\n                      </td>\r\n                      <td>{{ data.create_by }}</td>\r\n                      <td>{{ data.annotation }}</td>\r\n                      <td>{{ data.ref }}</td>\r\n                    </tr>\r\n                  </tbody>\r\n                  <tbody>\r\n                    <tr v-if=\"depositreport.length == '0'\">\r\n                      <td colspan=\"13\" class=\"text-center text-danger\">\r\n                        <i class=\"fas fa-times\"></i>\r\n                        ไม่มีข้อมูล !\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n            <!-- modal -->\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nconst moment = require(\"moment\");\r\nimport { baseURL } from \"../../../../services/api\";\r\nimport UserService from \"@/services/UserService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      datetime1: \"\",\r\n      datetime2: \"\",\r\n      true: true,\r\n      false: false,\r\n      bank: \"\",\r\n      amount: \"\",\r\n      bonus_credit: \"\",\r\n      transaction_type: \"\",\r\n      credit_before: \"\",\r\n      credit_after: \"\",\r\n      create_by: \"\",\r\n      annotation: \"\",\r\n      ref: \"\",\r\n      transaction_status: \"\",\r\n      createdAt: \"\",\r\n      updatedAt: \"\",\r\n      depositreport: [],\r\n      todaydate: \"\",\r\n      todaytime: \"\",\r\n      yesterdate: \"\",\r\n      yestertime: \"\",\r\n      todate: \"\",\r\n      totime: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.moment();\r\n    this.yesterdate = moment()\r\n      .locale(\"th\")\r\n\r\n      .format(\"YYYY-MM-DD\");\r\n    this.yestertime = moment()\r\n      .locale(\"th\")\r\n\r\n      .format(\"00:00:00\");\r\n    this.todate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n    this.totime = moment().locale(\"th\").format(\"23:59:59\");\r\n    this.todaydate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n    this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n    this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n    const t = \"T\";\r\n    const z = \"\";\r\n    const start_date = this.yesterdate + t + this.todaytime + z;\r\n    const end_date = this.todaydate + t + this.todaytime2 + z;\r\n\r\n    axios({\r\n      method: \"get\",\r\n      url: `${baseURL}/report/deposit/?start_date=${start_date}&end_date=${end_date}&username=`,\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n      },\r\n    })\r\n      .then((response) => {\r\n        this.depositreport = response.data.data;\r\n        setTimeout(async function () {\r\n          await axios();\r\n        }, 120000);\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n  },\r\n  methods: {\r\n    async checkAuth() {\r\n      const res = await UserService.getProfile().catch(function (error) {\r\n        if (error.response.status === 401 || error.response.status === 500) {\r\n          const removeToken = localStorage.removeItem(\"access_token\");\r\n          if (removeToken == null) {\r\n            this.$router.push(\"/login\");\r\n          }\r\n        }\r\n      });\r\n      res.logout;\r\n    },\r\n\r\n    moment() {\r\n      this.checkAuth();\r\n\r\n      return moment();\r\n    },\r\n\r\n    getDataParam() {\r\n      const username = this.username;\r\n      let s1 = moment(`${this.yesterdate}`).format(\"YYYY-MM-DDT\");\r\n      let e1 = moment(`${this.todate}`).format(\"YYYY-MM-DDT\");\r\n      let s2 = moment().format(`${this.yestertime}`);\r\n      let e2 = moment().locale(\"th\").format(`${this.totime}`);\r\n      // let a=  moment(`${this.totime}`).locale(\"TH\").format(\"HH:mm:ss\");\r\n      // let e3= moment.format();\r\n      // let s3= moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n      // let s3= moment();\r\n      // let s= moment().format();\r\n      // let a= moment.utc().format();\r\n\r\n      const start_date = s1 + s2;\r\n      const end_date = e1 + e2;\r\n      axios({\r\n        method: \"get\",\r\n        url: `${baseURL}/report/deposit/?start_date=${start_date}&end_date=${end_date}&username=${username}`,\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n        },\r\n      })\r\n        .then((response) => {\r\n          this.depositreport = response.data.data;\r\n          if (this.depositreport.length <= 0) {\r\n            this.$toast.error(\"ไม่พบรหัสข้อมูลนี้\", {\r\n              position: \"top-right\",\r\n              timeout: 1500,\r\n              icon: true,\r\n              rtl: false,\r\n            });\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getToday() {\r\n      this.todaydate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n      this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n      this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n      const t = \"T\";\r\n      const z = \"\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n      axios({\r\n        method: \"get\",\r\n        url: `${baseURL}/report/deposit/?start_date=${start_date}&end_date=${end_date}&username=`,\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n        },\r\n      })\r\n        .then((response) => {\r\n          this.depositreport = response.data.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getYesterday() {\r\n      this.todaydate = moment()\r\n        .locale(\"th\")\r\n        .subtract(1, \"days\")\r\n        .format(\"YYYY-MM-DD\");\r\n      this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n      this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n      const t = \"T\";\r\n      const z = \"\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n      axios({\r\n        method: \"get\",\r\n        url: `${baseURL}/report/deposit/?start_date=${start_date}&end_date=${end_date}&username=`,\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n        },\r\n      })\r\n        .then((response) => {\r\n          \r\n          console.log('response')\r\n          console.log(response)\r\n\r\n          this.depositreport = response.data.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getClipboard() {\r\n      this.$toast.success(\"คัดลอก User สำเร็จแล้ว\", {\r\n        position: \"top-right\",\r\n        timeout: 1500,\r\n      });\r\n    },\r\n    getClipboardBank() {\r\n      this.$toast.info(\"คัดลอก เลขบัญชี สำเร็จแล้ว\", {\r\n        position: \"top-right\",\r\n        timeout: 1500,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components/pages/report/report_deposit"}]}