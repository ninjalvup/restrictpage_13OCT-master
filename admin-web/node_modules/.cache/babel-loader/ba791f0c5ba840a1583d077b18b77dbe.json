A,QAAA,GAAA,KAAA,SAAA,GAAA,CAAA,GAAA,KAAA,UAAA,GAAA,CAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,YAAA,OAAA,uEAAA,UAAA,uBAAA,QAAA,eAFA;AAGA,QAAA,OAAA,EAAA;AACA,0BAAA,kBADA;AAEA,UAAA,aAAA,EAAA,YAAA,YAAA,CAAA,OAAA,CAAA,cAAA;AAFA;AAHA,OAAA,CAAA,CAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAbA;AAcA,KA9FA;AAgGA,IAAA,YAhGA,0BAgGA;AAAA;;AACA,WAAA,SAAA,GAAA,OAAA,GACA,MADA,CACA,IADA,EAEA,QAFA,CAEA,CAFA,EAEA,MAFA,EAGA,MAHA,CAGA,YAHA,CAAA;AAIA,WAAA,UAAA,GAAA,OAAA,GAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AACA,WAAA,SAAA,GAAA,OAAA,GAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AACA,UAAA,CAAA,GAAA,GAAA;AACA,UAAA,CAAA,GAAA,OAAA;AACA,UAAA,UAAA,GAAA,KAAA,SAAA,GAAA,CAAA,GAAA,KAAA,SAAA,GAAA,CAAA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,GAAA,CAAA,GAAA,KAAA,UAAA,GAAA,CAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,YAAA,OAAA,uEAAA,UAAA,uBAAA,QAAA,eAFA;AAGA,QAAA,OAAA,EAAA;AACA,0BAAA,kBADA;AAEA,UAAA,aAAA,EAAA,YAAA,YAAA,CAAA,OAAA,CAAA,cAAA;AAFA;AAHA,OAAA,CAAA,CAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAbA;AAcA,KAzHA;AA0HA,IAAA,gBA1HA,8BA0HA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,4BAAA,EAAA;AACA,QAAA,QAAA,EAAA,WADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA;AAIA,KA/HA;AAgIA,IAAA,YAhIA,0BAgIA;AACA,WAAA,MAAA,CAAA,OAAA,CAAA,wBAAA,EAAA;AACA,QAAA,QAAA,EAAA,WADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA;AAIA;AArIA;AAvEA,CAAA","sourcesContent":["<template>\r\n  <div id=\"User\">\r\n    <div class=\"page-inner\">\r\n      <div class=\"page-header\">\r\n        <h4 class=\"page-title\">\r\n          <i class=\"fas fa-user\"></i> รายการถอนที่ไม่ได้อนุมัติ\r\n        </h4>\r\n      </div>\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header\">\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <h3 class=\"card-title\">ข้อมูลที่ไม่ได้อนุมัติ: การถอน</h3>\r\n                </div>\r\n              </div>\r\n            </div>\r\n  <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"card\">\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">จากวันที่</label>\r\n                  <VueCtkDateTimePicker\r\n                    :only-date=\"true\"\r\n                    v-model=\"yesterdate\"\r\n                    format=\"YYYY-MM-DD\"\r\n                    formatted=\"DD-MM-YYYY\"\r\n                    :no-label=\"true\"\r\n                    locale=\"th-TH\"\r\n                    Label=\"yesterday\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">เวลา</label>\r\n                  <VueCtkDateTimePicker\r\n                    v-model=\"yestertime\"\r\n                    :only-time=\"true\"\r\n                    format=\"HH:mm:ss\"\r\n                    formatted=\"HH:mm\"\r\n                    :no-label=\"true\"\r\n                    label=\"Time\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">ถึงวันที่</label>\r\n                  <VueCtkDateTimePicker\r\n                    :only-date=\"true\"\r\n                    v-model=\"todate\"\r\n                    format=\"YYYY-MM-DD\"\r\n                    formatted=\"DD-MM-YYYY\"\r\n                    :no-label=\"true\"\r\n                    locale=\"th-TH\"\r\n                    Label=\"today\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">เวลา</label>\r\n                  <VueCtkDateTimePicker\r\n                    v-model=\"totime\"\r\n                    :only-time=\"true\"\r\n                    format=\"HH:mm:ss\"\r\n                    formatted=\"HH:mm\"\r\n                    :no-label=\"true\"\r\n                    label=\"Time\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-2\">\r\n                  <label for=\"datetime\">กรุณากรอกข้อมูลที่ต้องการ</label>\r\n                  <input\r\n                    class=\"form-control\"\r\n                    id=\"email2\"\r\n                    placeholder=\"ข้อมูลที่ต้องการค้นหา\"\r\n                    v-model=\"username\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-1.5\">\r\n                  <label for=\"datetime\">ตัวเลือก</label>\r\n                  <br />\r\n                  <button\r\n                    class=\"btn btn-primary btn-sm ml-1\"\r\n                    v-on:click.prevent=\"getDataParam()\"\r\n                  >\r\n              \r\n                    ค้นหา\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-secondary btn-sm ml-1\"\r\n                    v-on:click.prevent=\"getYesterday()\"\r\n                  >\r\n                    เมื่อวาน\r\n                  </button>\r\n                  <button\r\n                    class=\"btn btn-warning btn-sm ml-1\"\r\n                    v-on:click.prevent=\"getToday()\"\r\n                  >\r\n                    วันนี้\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n            <div class=\"card-body\">\r\n              <div class=\"table-responsive\">\r\n                <table id=\"basic-datatables\" class=\"display table table-striped table-hover\">\r\n                  <thead style=\"text-align:center\">\r\n                    <tr>\r\n                     <th>ธนาคาร</th>\r\n                      <th>ชื่อผู้ใช้</th>\r\n                      <th>ยอดเงิน</th>\r\n                      <th>โบนัส</th>\r\n                      <th>ประเภท</th>\r\n                      <th>เครดิตก่อนเติม</th>\r\n                      <th>เครดิตหลังเติม</th>\r\n                      <th>ทำรายการโดย</th>\r\n                      <th>หมายเหตุ</th>\r\n                      <th>Ref.</th>\r\n                      <th>สถานะ</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody style=\"text-align:center\">\r\n                    <tr v-for=\"data in withdrawpending\" v-bind:key=\"data.id\">\r\n                      <td>\r\n                        <div class=\"avatar mt-3\">\r\n                          <img\r\n                            :src=\"require(`../../../assets/bank_icon/${data.member_account_banks[0].bank_name.toLowerCase()}.png`)\"\r\n                            class=\"icon\"\r\n                            width=\"40\"\r\n\r\n                          />\r\n                        </div>\r\n                        <div><b>\r\n                            <a\r\n                              href=\"#/withdrawdisapprove\"\r\n                              @click=\"getClipboardBank\"\r\n                              v-clipboard:copy=\"\r\n                                data.member_account_banks[0].bank_number\r\n                              \"\r\n                              >{{ data.member_account_banks[0].bank_number }}</a\r\n                            ></b\r\n                          ></div>\r\n                        <div>{{data.member_account_banks[0].bank_account_name}}</div>\r\n                      </td>\r\n                      <td> <b>\r\n                          <a\r\n                            href=\"#/withdrawdisapprove\"\r\n                            @click=\"getClipboard\"\r\n                            v-clipboard:copy=\"data.username\"\r\n                            >{{ data.username }}</a\r\n                          ></b\r\n                        ></td>\r\n                      <td>{{data.amount | formatCredit}}</td>\r\n                      <td>{{data.bonus_credit}}</td>\r\n                      <td>\r\n                        <div v-if=\"data.transaction_type =='1'\">\r\n                          <span class=\"badge badge-info\">การฝาก</span>\r\n                        </div>\r\n                        <div v-if=\"data.transaction_type =='2'\">\r\n                          <span class=\"badge badge-warning\">การถอน</span>\r\n                        </div>\r\n                      </td>\r\n                      <td>{{data.credit_before | formatCredit}}</td>\r\n                      <td>{{data.credit_after | formatCredit}}</td>\r\n                      <td>{{data.create_by}}</td>\r\n                      <td>{{data.annotation}}</td>\r\n                      <td>{{data.ref}}</td>\r\n                      <td>\r\n                        <div v-if=\"data.transaction_status =='1'\">\r\n                          <span class=\"badge badge-success\">อนุมัติแล้ว</span>\r\n                        </div>\r\n                        <div v-if=\"data.transaction_status =='2'\">\r\n                          <span class=\"badge badge-danger\">ไม่ได้อนุมัติ</span>\r\n                        </div>\r\n                      </td>\r\n                     \r\n                    </tr>\r\n                  </tbody>\r\n                   <tbody>\r\n                    <tr v-if=\"withdrawpending.length =='0'\">\r\n                      <td colspan=\"14\" class=\"text-center text-danger\">\r\n                        <i class=\"fas fa-times\"></i>\r\n                        ไม่มีข้อมูล !\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport managewithdrawService from \"@/services/managewithdrawService\";\r\nimport { baseURL } from '../../../services/api';\r\n\r\nconst url = `${baseURL}/member_transaction`;\r\n\r\nconst moment = require(\"moment\");\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n       datetime: \"\",\r\n      true: true,\r\n      false: false,\r\n      bank: \"\",\r\n      username: \"\",\r\n      amount: \"\",\r\n      bonus_credit: \"\",\r\n      transaction_type: \"\",\r\n      credit_before: \"\",\r\n      credit_after: \"\",\r\n      create_by: \"\",\r\n      annotation: \"\",\r\n      ref: \"\",\r\n      transaction_status: \"\",\r\n      createdAt: \"\",\r\n      updatedAt: \"\",\r\n\r\n      withdrawpending: [],\r\n      withdrawbyuuid: [],\r\n      credit: \"\",\r\n      amountuuid: \"\",\r\n      usernameuuid: \"\",\r\n      form: {},\r\n        todaydate: \"\",\r\n      todaytime: \"\",\r\n        yesterdate: \"\",\r\n      yestertime: \"\",\r\n       todate: \"\",\r\n      totime: \"\",\r\n    };\r\n  },\r\n async mounted() {\r\n\r\n     this.moment();\r\n    this.yesterdate = moment()\r\n      .locale(\"th\")\r\n      .subtract(1, \"days\")\r\n      .format(\"YYYY-MM-DD\");\r\n    this.yestertime = moment()\r\n      .locale(\"th\")\r\n      .subtract(1, \"days\")\r\n      .format(\"11:00:00\");\r\n    this.todate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n    this.totime = moment().locale(\"th\").format(\"11:00:00\");\r\n    this.todaydate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n    this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n    this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n    const t = \"T\";\r\n    const z = \".000Z\";\r\n    const start_date = this.todaydate + t + this.todaytime + z;\r\n    const end_date = this.todaydate + t + this.todaytime2 + z;\r\n\r\n  await  axios({\r\n      method: \"get\",\r\n      url: `${baseURL}/member_transaction/withdraw-dis-approve-list/?start_date=${start_date}&end_date=${end_date}&username=`,\r\n      headers: {\r\n        \"Content-Type\": \"application/json\",\r\n        Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n      },\r\n    })\r\n      .then(( response) => {\r\n        this.withdrawpending =  response.data.data;\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n\r\n  },\r\n  methods: {\r\nmoment() {\r\n      return moment();\r\n    },\r\n \r\n\r\n    async getShowWithdrawByUuid(uuid) {\r\n      const res = await managewithdrawService.getWithdrawByUuid(uuid);\r\n      this.withdrawbyuuid = res.data;\r\n      this.amountuuid = res.data.withdraw.amount;\r\n      this.credit = res.data.current_credit.credit;\r\n      this.usernameuuid = res.data.withdraw.username;\r\n\r\n    },\r\n\r\n\r\n    checkClicked() {\r\n      this.$confirm(\"ยืนยันการทำรายการ\").then(() => {\r\n        this.postAccept();\r\n      });\r\n    },\r\n\r\n    async postAccept() {\r\n     await axios\r\n        .post(url + \"/approved-withdraw\", this.withdrawbyuuid.withdraw, {\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n          },\r\n        })\r\n        .then(() => {\r\n          this.$toast.success(\"สมัครเรียบร้อยแล้ว\", {\r\n            position: \"top-right\",\r\n            timeout: 1500,\r\n            icon: true,\r\n            rtl: false,\r\n          });\r\n          //Perform Success Action\r\n        });\r\n    },\r\n    \r\n\r\n    getDataParam() {\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const username = this.username;\r\n      const start_date = this.yesterdate + t + this.yestertime + z;\r\n      const end_date = this.todate + t + this.totime + z;\r\n      axios({\r\n        method: \"get\",\r\n        url: `${baseURL}/member_transaction/withdraw-dis-approve-list/?start_date=${start_date}&end_date=${end_date}&username=${username}`,\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n        },\r\n      })\r\n        .then((response) => {\r\n          this.withdrawpending = response.data.data;\r\n          if (this.withdrawpending.length <= 0) {\r\n            this.$toast.error(\"ไม่พบรหัสข้อมูลนี้\", {\r\n              position: \"top-right\",\r\n              timeout: 1500,\r\n              icon: true,\r\n              rtl: false,\r\n            });\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getToday() {\r\n      this.todaydate = moment().locale(\"th\").format(\"YYYY-MM-DD\");\r\n      this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n      this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n      axios({\r\n        method: \"get\",\r\n        url: `${baseURL}/member_transaction/withdraw-dis-approve-list/?start_date=${start_date}&end_date=${end_date}&username=`,\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n        },\r\n      })\r\n        .then((response) => {\r\n          this.withdrawpending = response.data.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    getYesterday() {\r\n      this.todaydate = moment()\r\n        .locale(\"th\")\r\n        .subtract(1, \"days\")\r\n        .format(\"YYYY-MM-DD\");\r\n      this.todaytime2 = moment().locale(\"th\").format(\"23:59:59\");\r\n      this.todaytime = moment().locale(\"th\").format(\"00:00:00\");\r\n      const t = \"T\";\r\n      const z = \".000Z\";\r\n      const start_date = this.todaydate + t + this.todaytime + z;\r\n      const end_date = this.todaydate + t + this.todaytime2 + z;\r\n      axios({\r\n        method: \"get\",\r\n        url: `${baseURL}/member_transaction/withdraw-dis-approve-list/?start_date=${start_date}&end_date=${end_date}&username=`,\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: \"Bearer \" + localStorage.getItem(\"access_token\"),\r\n        },\r\n      })\r\n        .then((response) => {\r\n          this.withdrawpending = response.data.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getClipboardBank() {\r\n      this.$toast.info(\"คัดลอก เลขบัญชี สำเร็จแล้ว\", {\r\n        position: \"top-right\",\r\n        timeout: 1500,\r\n      });\r\n    },\r\n    getClipboard() {\r\n      this.$toast.success(\"คัดลอก User สำเร็จแล้ว\", {\r\n        position: \"top-right\",\r\n        timeout: 1500,\r\n      });\r\n    },\r\n  \r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/pages/manageWithdraw"}]}