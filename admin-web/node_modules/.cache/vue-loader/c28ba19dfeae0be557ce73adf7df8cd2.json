A;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Under-right.vue","sourceRoot":"src/components/pages/Dashboard","sourcesContent":["<template>\r\n  <div id=\"Under-right\">\r\n    <div class=\"card\">\r\n      <div class=\"card-header bg-primary\">\r\n        <h3 class=\"card-title text-white\">รายการรออนุมัติ</h3>\r\n      </div>\r\n      <div v-if=\"smstable.length == '0'\">\r\n        <div class=\"alert alert-danger mt-4 mb-3 m-4\" role=\"alert\">\r\n          ไม่มีข้อมูลรายการอนุมัติ\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"panel-body page-main-body-transaction mt-2 mr-2 ml-2\"\r\n        v-for=\"data in smstable\"\r\n        v-bind:key=\"data.data\"\r\n      >\r\n        <!-- ngRepeat: i in result -->\r\n        <table class=\"table table-striped\" style=\"\">\r\n          <tbody>\r\n            <tr\r\n              style=\"\r\n                color: black;\r\n                border: solid 0px #c1c1c1;\r\n                height: 70px;\r\n                background-color: #ffffff;\r\n              \"\r\n            >\r\n              <!-- <td style=\"width: 55px\">\r\n                <img\r\n                  :src=\"\r\n                    require(`../../../assets/bank_icon/${data.bank_tranfer.toLowerCase()}.png`)\r\n                  \"\r\n                  alt\r\n                  class=\"icon ml-3\"\r\n                  width=\"40\"\r\n                  height=\"40\"\r\n                />\r\n                 </td> -->\r\n              <td></td>\r\n              <td style=\"width: 55px\">\r\n                <img\r\n                  :src=\" require(`../../../../public/assets/img/bank_icon/${data.bank_tranfer.toLowerCase()}.png`) \"\r\n                  class=\"icon ml-3\"\r\n                  width=\"40\"\r\n                  height=\"40\"\r\n                />\r\n              </td>\r\n              <td style=\"width: 430px\">\r\n                <small\r\n                  >ลูกค้าโอนเข้าแต่ไม่พบข้อมูล\r\n                  กรุณารอลูกค้าติดต่อมาเพื่อทำรายการ</small\r\n                >\r\n                <br />\r\n                <!-- <small ><i class=\"fa fa-clock-o\"></i>{{data.tel}} -->\r\n                <!-- </small> -->\r\n                <br />\r\n                {{ data.sms_content }}\r\n                <br />\r\n                <b></b>\r\n              </td>\r\n              <td>\r\n                <small>ช่องทาง</small>\r\n                <br />\r\n                <b>SMS</b>\r\n\r\n                <!-- <b v-if=\"data.title_tranfer === null\">SMS</b> -->\r\n                <!-- <p class=\"badge badge-secondary\" >{{ data.bank_tranfer.toUpperCase() }}</p> -->\r\n              </td>\r\n              <td>\r\n                <small>จำนวนเงิน</small>\r\n                <br />\r\n                <b class=\"badge badge-secondary\"> {{ data.amount }}</b>\r\n              </td>\r\n              <!-- ngIf: user.position !== 'STAFF' -->\r\n              <td class=\"text-right\">\r\n                <!-- <button class=\"btn btn-default btn-secondary btn-sm mt-1 mb-1\" ><i class=\"fa fa-check\"></i> เติม</button>\r\n                                                <div style=\"padding: 2px;\"></div>\r\n                                                <button class=\"btn btn-primary btn-sm mt-1 mb-1\" ><i class=\"fa fa-times\"></i> ซ่อน</button> -->\r\n                <div v-if=\"data.bank_tranfer.toLowerCase() != 'truemoney'\">\r\n                  <button\r\n                    class=\"\r\n                      btn btn-secondary\r\n                      ml-3\r\n                      mt-2\r\n                      mb-2\r\n                      mr-3\r\n                      btn-sm\r\n                      text-white\r\n                      float-right\r\n                    \"\r\n                    data-toggle=\"modal\"\r\n                    data-target=\"#postCurl\"\r\n                    v-on:click.prevent=\"getSMSByUUID(data.uuid)\"\r\n                  >\r\n                    <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                </div>\r\n                <div v-if=\"data.bank_tranfer.toLowerCase() === 'truemoney'\">\r\n                  <button\r\n                    class=\"\r\n                      btn btn-secondary\r\n                      ml-3\r\n                      mr-3\r\n                      btn-sm\r\n                      text-white\r\n                      float-right\r\n                    \"\r\n                    data-toggle=\"modal\"\r\n                    data-target=\"#postTrue\"\r\n                    v-on:click.prevent=\"getTruemoneyID(data.uuid)\"\r\n                  >\r\n                    <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                </div>\r\n                <div v-if=\"data.bank_tranfer === null\">\r\n                  <button\r\n                    class=\"\r\n                      btn btn-secondary\r\n                      ml-3\r\n                      mr-3\r\n                      btn-sm\r\n                      text-white\r\n                      float-right\r\n                    \"\r\n                    data-toggle=\"modal\"\r\n                    data-target=\"#postTrue\"\r\n                    v-on:click.prevent=\"getTruemoneyID(data.uuid)\"\r\n                  >\r\n                    <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\r\n                  </button>\r\n                </div>\r\n                <button\r\n                  v-on:click.prevent=\"postToHideSMS(data.uuid)\"\r\n                  class=\"\r\n                    btn btn-danger btn-sm\r\n                    mr-3\r\n                    mt-1\r\n                    mb-1\r\n                    text-white\r\n                    float-right\r\n                  \"\r\n                >\r\n                  <i class=\"fa fa-times-circle\" aria-hidden=\"true\"></i>\r\n                </button>\r\n              </td>\r\n              <!-- end ngIf: user.position !== 'STAFF' -->\r\n            </tr>\r\n            <!-- end ngIf: i.bankName == 'SCB' -->\r\n          </tbody>\r\n        </table>\r\n        <!-- end ngRepeat: i in result -->\r\n        <hr />\r\n      </div>\r\n\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"postTrue\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"myExtraLargeModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <!-- MODAL -->\r\n        <!-- v-on:submit.prevent=\"checkClickedSMS(uuid)\" -->\r\n        <form>\r\n          <div class=\"modal-dialog modal-dialog-centered\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"exampleModalLabel\">\r\n                  ข้อมูลSMS TRUE\r\n                </h5>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"close\"\r\n                  data-dismiss=\"modal\"\r\n                  aria-label=\"Close\"\r\n                >\r\n                  <span aria-hidden=\"true\">&times;</span>\r\n                </button>\r\n              </div>\r\n\r\n              <div class=\"card-body\">\r\n                <div class=\"d-flex\">\r\n                  <div class=\"avatar\">\r\n                    <img\r\n                      :src=\"\r\n                        require(`../../../assets/bank_icon/${titletrue.toLowerCase()}.png`)\r\n                      \"\r\n                      alt\r\n                      class=\"icon\"\r\n                      width=\"40\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"flex-1 ml-3 pt-1 container\">\r\n                    <span class=\"text-uppercase fw-bold mb-1\"\r\n                      >จำนวนเงิน\r\n\r\n                      <h2 class=\"text-warning pl-3\">\r\n                        {{ amounttrue }}\r\n                      </h2>\r\n                    </span>\r\n                  </div>\r\n                  <div class>\r\n                    <p class=\"text-muted\">\r\n                      {{ bank_datetrue }} {{ bank_timetrue }}\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                  <label for=\"username\">ชื่อผู้ใช้</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    aria-describedby=\"helpId\"\r\n                    name=\"username\"\r\n                    id=\"username\"\r\n                    placeholder=\"กรุณากรอก ชื่อผู้ใช้\"\r\n                    required\r\n                    v-model=\"form.username\"\r\n                  />\r\n                </div>\r\n\r\n                <button\r\n                  class=\"btn btn-info mr-1 text-white float-right\"\r\n                  v-on:click.prevent=\"postTrue(uuid)\"\r\n                >\r\n                  <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\r\n\r\n                  ยืนยัน\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n\r\n      <div\r\n        class=\"modal fade\"\r\n        id=\"postCurl\"\r\n        tabindex=\"-1\"\r\n        role=\"dialog\"\r\n        aria-labelledby=\"myExtraLargeModalLabel\"\r\n        aria-hidden=\"true\"\r\n      >\r\n        <form>\r\n          <div class=\"modal-dialog modal-dialog-centered\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"exampleModalLabel\">ข้อมูลSMS</h5>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"close\"\r\n                  data-dismiss=\"modal\"\r\n                  aria-label=\"Close\"\r\n                >\r\n                  <span aria-hidden=\"true\">&times;</span>\r\n                </button>\r\n              </div>\r\n\r\n              <div class=\"card-body\">\r\n                <div class=\"d-flex\">\r\n                  <div class=\"avatar\">\r\n                    <img\r\n                      :src=\"\r\n                        require(`../../../assets/bank_icon/${titlesms.toLowerCase()}.png`)\r\n                      \"\r\n                      alt\r\n                      class=\"icon\"\r\n                      width=\"40\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"flex-1 ml-3 pt-1 container\">\r\n                    <span class=\"text-uppercase fw-bold mb-1\"\r\n                      >จำนวนเงิน\r\n\r\n                      <h2 class=\"text-warning pl-3\">\r\n                        {{ amountsms }}\r\n                      </h2>\r\n                    </span>\r\n                  </div>\r\n                  <div class>\r\n                    <p class=\"text-muted\">\r\n                      {{ bank_datesms }} {{ bank_timesms }}\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                  <label for=\"username\">ชื่อผู้ใช้</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    aria-describedby=\"helpId\"\r\n                    name=\"username\"\r\n                    id=\"username\"\r\n                    placeholder=\"กรุณากรอก ชื่อผู้ใช้\"\r\n                    required\r\n                    v-model.trim=\"form.username\"\r\n                  />\r\n                </div>\r\n\r\n                <button\r\n                  class=\"btn btn-info mr-1 text-white float-right\"\r\n                  id=\"postSMS\"\r\n                  v-on:click.prevent=\"postSMS(uuid)\"\r\n                >\r\n                  <i class=\"fa fa-check\" aria-hidden=\"true\"></i>\r\n\r\n                  ยืนยัน\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-4\">\r\n      <audio id=\"myAudio\" hidden controls autoplay>\r\n        <source src=\"../../../assets/audio/sms.mp3\" type=\"audio/mpeg\" />\r\n        Your browser does not support the audio element.\r\n      </audio>\r\n\r\n      <!-- <button onclick=\"myFunction()\">Try it</button> -->\r\n\r\n      <!-- <p id=\"demo\"></p> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\n// import bankService from \"@/services/bankService\";\r\nimport swal from \"sweetalert\";\r\nimport UserService from \"@/services/UserService\";\r\nimport smsService from \"@/services/smsService\";\r\n// import axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      content: \"\",\r\n      bank_datesms: \"\",\r\n      bank_timesms: \"\",\r\n      amountsms: \"\",\r\n      // decimalbank: \"\",\r\n      titlecurl: \"curl\",\r\n      titlecurl_kbank: \"curl_kbank\",\r\n      titlesms: \"SCBA\",\r\n      titletrue: \"Truemoney\",\r\n\r\n      bank_datecurl: \"\",\r\n      bank_timecurl: \"\",\r\n      amountcurl: \"\",\r\n\r\n      bank_datetrue: \"\",\r\n      bank_timetrue: \"\",\r\n      amounttrue: \"\",\r\n\r\n      //   bank_datesms: \"\",\r\n      // bank_timesms: \"\",\r\n      // amountsms: \"\",\r\n\r\n      form: {\r\n        username: \"\",\r\n      },\r\n      allsmstable: [],\r\n      allcurlnoacc: [],\r\n      smstable: [],\r\n      alltruetable: [],\r\n      status0test: [],\r\n      smsForm: {\r\n        amount: \"\",\r\n        username: \"\",\r\n        bank_date: \"\",\r\n        bank_time: \"\",\r\n        create_by: \"\",\r\n      },\r\n      // ishasData: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    // this.getSMS()\r\n    this.getWait();\r\n    this.pauseAudio();\r\n    let audio = new Audio(require(\"../../../assets/audio/sms.mp3\"));\r\n    audio.pause();\r\n  },\r\n  methods: {\r\n    //     async getSMS() {\r\n    //       this.allsmstable = [];\r\n    //       this.smstable = [];\r\n    //       // let k = 0;\r\n    //       const res = await smsService.getScbSmsTransaction();\r\n    //       console.log(res.data.status);\r\n    //       for (var i = 0; i < res.data.length; i++) {\r\n\r\n    //         if (res.data[i].status === \"0\") {\r\n\r\n    //           this.smstable.push({\r\n    //             id: res.data[i].id,\r\n    //             uuid: res.data[i].uuid,\r\n    //             title: res.data[i].bank_tranfer, //\r\n    //             amount: res.data[i].amount, //\r\n    //             status: res.data[i].status,\r\n    //             tel: res.data[i].phone_number,\r\n    //             date: res.data[i].bank_date, //\r\n    //             time: res.data[i].bank_time, //\r\n    //             content:\r\n    //               res.data[i].sms_content, //\r\n    //           });\r\n    //         }\r\n\r\n    //       }\r\n    // console.log(\"OLD \"+this.smstable.length);\r\n    //       this.smstable.forEach((element) =>  {\r\n\r\n    //         this.allsmstable.push(element);\r\n\r\n    //       });\r\n    //       // console.log(\"OLD \"+this.allsmstable.length);\r\n\r\n    //       let self = this;\r\n    //       setTimeout(async function () {\r\n    //         await self.getSMS();\r\n    //       }, 30000);\r\n    //       if (this.allsmstable.